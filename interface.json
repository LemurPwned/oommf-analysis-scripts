{
  "name": "Voltage spin-diode frequency analysis module",
  "description": "Allows to perform analysis on vsd systems",
  "specification": [
      {
          "name": "directory",
          "short": "d",
          "help": "directory to be investigated",
          "type": "str"
      },
      {
          "name": "param_name",
          "short": "pn",
          "help": "param to be investigated during sweep",
          "type": "str"
      },
      {
        "name": "resonant_frequency",
        "short": "rf",
        "help": "sets resonant frequency",
        "type": "float"
      },
      {
          "name": "time_step",
          "short": "ts",
          "help": "sampling rate",
          "type": "float"
      },
      {
          "name": "start_time",
          "short": "tstart",
          "help": "starts investigation of data from tstart",
          "type": "float"
      },
      {
          "name": "stop_time",
          "short": "tstop",
          "help": "stops investigation of data at tstop",
          "type": "float"
      },
      {
        "name": "dispersion",
        "short": "disp",
        "help": "dispersion analysis enable",
        "type": "bool",
        "action": "store_true"
      },
      {
        "name": "view",
        "short": "v",
        "help": "view current default_dict",
        "type": "bool",
        "action": "store_true"
      },
      {
        "name": "clear",
        "short": "c",
        "help": "ignores saved objects and overwrites them",
        "type": "bool",
        "action": "store_true"
      }
    ]
}
